# irmovq $10, %rdi     #jaettava
# irmovq $-6, %rsi     #jakaja
# rax = tulos

jakolaskuu:
    irmovq $1, %rdx  #tallennetaan 1
    addq %rsi, %rcx #onko 0
    je donej        #onko 0
    irmovq $0, %rcx
    addq %rsi, %rcx
    jl poistamerkkijakaja
    irmovq $0, %rcx
    addq %rdi, %rcx
    jl poistamerkkijaettava
    jmp jakolasku

jakolasku:
    subq %rsi, %rdi
    jl valmis
    addq %rdx, %rax   #lisätään 1
    jmp jakolasku
    
poistamerkkijakaja:
    subq %rsi, %rbx
    rrmovq %rbx, %rsi
    irmovq $-1, %rsp   #merkataan rekisteriin, että on neg
    irmovq $0, %rcx
    addq %rdi, %rcx
    jl poistamerkkijaettava
    jmp jakolasku
    
poistamerkkijaettava:
    irmovq $0, %rbx
    subq %rdi, %rbx
    rrmovq %rbx, %rdi
    irmovq $-1, %rbp   #merkataan rekisteriin, että on neg
    jmp jakolasku
    
lisaamerkki:
    irmovq $0, %rbx
    subq %rax, %rbx
    rrmovq %rbx, %rax
    jmp donej
    
valmis:
    subq %rsp, %rbp
    je donej
    jmp lisaamerkki
    
donej:
    ret
